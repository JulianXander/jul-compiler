{
	"$schema": "http://json-schema.org/draft-07/schema#",
	"title": "jul compiler configuration",
	"description": "Configures jul compiler",
	"type": "object",
	"properties": {
		"entryFilePath": {
			"description": "Relative to config directory",
			"type": "string",
			"pattern": "\\.jul$"
		},
		"outputFolder": {
			"description": "Relative to config directory",
			"type": "string",
			"default": "out"
		}
	},
	"required": [
		"entryFilePath"
	],
	"additionalProperties": false
}