# Liefert die Summe der args.
add = nativeFunction(
	(...args: List(Rational)) :> Rational
	§
		(...args) =>
			args.reduce(
				(accumulator, current) =>
					accumulator + current,
				0)
	§
)
# Liefert die Summe der args.
addFloat = nativeFunction(
	(...args: List(Float)) :> Float
	§
		(...args) =>
