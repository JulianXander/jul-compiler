{
  "name": "jul-compiler",
  "version": "0.0.0",
  "description": "Compiler cli for JUL ",
  "main": "out/cli.js",
  "type": "module",
  "bin": {
    "jul": "out/cli.js"
  },
  "scripts": {
    "build": "npx tsc && copyfiles --flat src/core-lib.jul out",
    "build-all": "npm run build && npx tsc --project ../jul-language-server/tsconfig.json && npx tsc --project ../vscode-jul-language-service/tsconfig.json",
    "test": "tsc -p tsconfig.test.json && mocha 'test-setup.mjs' 'test/**/*.test.js'",
    "install-cli": "npm i -g"
  },
  "author": {
    "name": "Julian Xander"
  },
  "license": "ISC",
  "devDependencies": {
    "@types/chai": "^4.3.4",
    "@types/js-yaml": "^4.0.5",
    "@types/mocha": "^10.0.1",
    "@types/node": "^18.13.0",
    "chai": "^4.3.7",
    "chai-as-promised": "^7.1.1",
    "mocha": "^10.2.0",
    "ts-node": "^10.9.1"
  },
  "dependencies": {
    "ajv": "^6.12.6",
    "js-yaml": "^4.1.0",
    "typescript": "^4.9.5",
    "webpack": "^5.75.0"
  }
}